@model Pokemon

<h1>Registrar ciudad y entrenador</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="NombreCiudad"></label>
        <input asp-for="NombreCiudad" class="form-control">
        <label asp-for="Foto"></label>
        <input asp-for="Foto" class="form-control">
    </div>
<br>
    <div class="form-group">
        <label asp-for="NombreEntrenador"></label>
        <input asp-for="NombreEntrenador" class="form-control">
        <label asp-for="Foto"></label>
        <input asp-for="Foto" class="form-control">
    </div>

    <button class="btn btn-primary">Enviar</button>

</form>