@model List<Pokemon>

<h1>Lista las ciudades pokemon con sus respectivos entrenadores de gimnasio</h1>

@if (Model.Count == 0) {
    <div class="alert alert-warning text-center">NO HAY NADA.</div>
} else {
    <ol>
    @foreach (var p in Model) {
        <li class="d-flex">
            <div>@p.NombreCiudad</div>
            <div><img src="@p.Foto" /></div>
            <div>@p.NombreEntrenador</div>
            <div><img src="@p.Foto" /></div>
        </li>
    }
    </ol>
}